{
  "name": "gatsby-netlify-todos",
  "description": "A simple todos app built with Gatsby & Netlify Functions",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Devon Bellizio",
  "repository": {
    "type": "git",
    "url": "https://github.com/bellizio/gatsby-netlify-todos"
  },
  "keywords": [
    "gatsby",
    "react",
    "netlify",
    "lambda",
    "serverless",
    "mongodb"
  ],
  "scripts": {
    "postinstall": "netlify-lambda install",
    "develop": "gatsby develop",
    "start": "npm run start:db && run-p start:lambda start:app",
    "start:app": "npm run develop",
    "start:lambda": "netlify-lambda serve src/api/functions",
    "start:db": "docker-compose up -d",
    "destroy:db": "docker-compose down",
    "build": "gatsby build && netlify-lambda build src/api/functions",
    "build:app": "gatsby build",
    "build:lambda": "netlify-lambda build src/api/functions"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.11",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/styles": "^4.9.10",
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "gatsby": "^2.20.25",
    "gatsby-image": "^2.3.4",
    "gatsby-plugin-manifest": "^2.3.5",
    "gatsby-plugin-offline": "^3.1.4",
    "gatsby-plugin-react-helmet": "^3.2.4",
    "gatsby-plugin-sass": "^2.2.3",
    "gatsby-plugin-sharp": "^2.5.6",
    "gatsby-source-filesystem": "^2.2.4",
    "gatsby-transformer-sharp": "^2.4.6",
    "mongoose": "^5.9.9",
    "node-sass": "^4.13.1",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "http-proxy-middleware": "^1.0.3",
    "netlify-lambda": "^1.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4"
  }
}
